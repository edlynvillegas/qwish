    "test:integration": "cross-env RUN_INTEGRATION_TESTS=true vitest run --project integration",
    "test:integration:watch": "cross-env RUN_INTEGRATION_TESTS=true vitest --project integration",
    "test:all": "cross-env RUN_INTEGRATION_TESTS=true vitest run"
